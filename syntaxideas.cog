const *eight* = 8;

// (include-book "std/util/defaggregate" :dir :system)

// (std::defaggregate point ((x (and (numberp x))) (y (and (numberp y)))))
struct point(
  x: number,
  y: number,
);

// (defun pretty-print-point (p)
//   (cw "[Point x=~x0 y=~x1]~%" (point->x p) (point->y p)))
function pretty-print-point (p: point) {
  print("[Point x=~x0 y=~x1]", p.x, p.y);
}

// (pretty-print-point (make-point :x 1/2 :y 3/2))
pretty-print-point(new point(x = 1/2, y = 3/2))